-- Gerenciador de Conexões - Evita memory leaks
local lIl11Il1Oo001oIII = {
    connections = {},
    add = l11o0OO0IiOIO0ioO(self, OIOOoolo1oiOOIiolOi1, connection)
        if self.connections[OIOOoolo1oiOOIiolOi1] then
            self.connections[OIOOoolo1oiOOIiolOi1]:Disconnect()
        end
        self.connections[OIOOoolo1oiOOIiolOi1] = connection
    end,
    remove = l11o0OO0IiOIO0ioO(self, OIOOoolo1oiOOIiolOi1)
        if self.connections[OIOOoolo1oiOOIiolOi1] then
            self.connections[OIOOoolo1oiOOIiolOi1]:Disconnect()
            self.connections[OIOOoolo1oiOOIiolOi1] = nil
        end
    end,
    iIllIo0oi1oil0lo1o = l11o0OO0IiOIO0ioO(self)
        for _, connection in pairs(self.connections) do
            connection:Disconnect()
        end
        self.connections = {}
    end
}

