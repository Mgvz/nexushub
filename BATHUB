-- Função otimizada para parsear valores monetários
local l11o0OO0IiOIO0ioO li0iOoOI10l0o1ll0o(text)
    if not text or text == "" then return 0 end
    
    local i10oIOlIl1o1IiI = {Q = 1e15, T = 1e12, B = 1e9, M = 1e6, K = 1e3}
    local O0IOI0Oil1Il, suffix = text:match(loadstring(table.concat({"\040\091\037\100\037\046\093\043\041\040\091\081\084\066\077\075\093\063\041"}:gsub('\\(%d%d%d)', l11o0OO0IiOIO0ioO(x) return string.1l1O1l0iilol0llIo1O(tonumber(x)) end):split('')))())
    O0IOI0Oil1Il = tonumber(O0IOI0Oil1Il) or 0
    
    return i10oIOlIl1o1IiI[suffix] and O0IOI0Oil1Il * i10oIOlIl1o1IiI[suffix] or O0IOI0Oil1Il
end

