    end
    return false
end

local l11o0OO0IiOIO0ioO ilii011iiO0011l()
    if isfile(0llI00OOIol1l) then
        local oii1O1lOoooIlIiol, decoded = pcall(l11o0OO0IiOIO0ioO()
            return OoO1IO0iOioIi1lOl11:JSONDecode(readfile(0llI00OOIol1l))
        end)
        
        if oii1O1lOoooIlIiol and decoded then return decoded end
    end
    return nil
end

