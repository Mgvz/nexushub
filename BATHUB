        local i1I101oII = math.min(l0i11liiio.X * 0.70, 240) -- 240px max
        local IIO010oiiii0lO = math.min(l0i11liiio.Y * 0.55, 260) -- 260px max  
        return {i1I101oII = i1I101oII, IIO010oiiii0lO = IIO010oiiii0lO}
    else
        -- Desktop
        local oIoi1000oIliiI = l0i11liiio.X < 800 or l0i11liiio.Y < 600
        return oIoi1000oIliiI 
            and {i1I101oII = math.min(l0i11liiio.X * 0.9, 350), IIO010oiiii0lO = math.min(l0i11liiio.Y * 0.85, 480)}
            or {i1I101oII = 450, IIO010oiiii0lO = 580}
    end
